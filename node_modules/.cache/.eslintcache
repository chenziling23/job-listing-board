ge\">\n            <Nav type = \"editjob\" />\n     \n\n        <div className = \"contain\">\n            <div className=\"job-finder\">\n                JobFinder\n            </div>\n\n            <div className = \"username\">\n                Title*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.title} onChange={(e) => {\n                const title = e.target.value;\n                console.log(title);\n                setJobData({\n                    ...jobData,\n                    title: title\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Company*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.company} onChange={(e) => {\n                const company = e.target.value;\n                setJobData({\n                    ...jobData,\n                    company: company\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Location*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.location} onChange={(e) => {\n                const location = e.target.value;\n                setJobData({\n                    ...jobData,\n                    location: location\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Description*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.description} onChange={(e) => {\n                const description = e.target.value;\n                setJobData({\n                    ...jobData,\n                    description: description\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Employer Email*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.employerEmail} onChange={(e) => {\n                const employerEmail = e.target.value;\n                setJobData({\n                    ...jobData,\n                    employerEmail: employerEmail\n                })\n            }}/>\n            </div>\n            <div className = \"username\">\n                Company Website(Optional):\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.web} onChange={(e) => {\n                const web = e.target.value;\n                setJobData({\n                    ...jobData,\n                    web: web\n                })\n            }}/>\n            </div>\n\n            <button onClick={editJob}>Submit Edit</button>\n           \n        </div>\n        </div>  \n          \n     \n    );\n}\n\nexport default EditJob","/Users/ziling/Desktop/job-listing-board/src/js/LoggedHome.jsx",["65"],"import React from 'react';\nimport { useState, useEffect } from 'react';\nimport axios from 'axios';\nimport \"../css/LoggedHome.css\";\nimport Nav from \"./Nav\";\nimport { Link } from 'react-router-dom';\nimport { useNavigate } from 'react-router';\n\n\nfunction LoggedHome() {\n  const navigate = useNavigate();\n  const [formInput, setFormInput] = useState('');\n\n  const [job, setJob] = useState([]);\n\n  function onSearchButtonClick() {\n    axios.get(\"/api/jobs/\" + formInput)\n      .then(response => {setJob(response.data)})\n      .catch(error => setJob({\n        title: \"No job found\",\n        company: \"No company found\", \n      }));\n  }\n\n  function checkLogin() {\n    axios.get('/api/users/whoIsLoggedIn')\n        .then((res) => console.log(res.data))\n        .catch(() => navigate('/logIn'))\n  }\n\n  useEffect(checkLogin,[])\n\n\n  const jobList = job.map(oneJob => {\n    if (oneJob === undefined) {\n      return (\n        <div></div>\n      )\n    }else{\n      return (\n            <div id = \"joblst\">\n            <Link to={\"/jobDetail/\"+oneJob._id}>\n            <div id = \"onejob\">\n              <p>Job Title: {oneJob.title}</p>\n              <p>Company: {oneJob.company}</p>\n              <p>Location: {oneJob.location}</p>\n            </div>\n            </Link>\n            <br/>\n            </div>)\n    }\n  })\n\n\n  return (\n    <div className=\"home\">\n      <Nav type = \"logged\"/>\n      <div className=\"container\">\n      <div className=\"header\">\n        JobFinder\n      <div className=\"put\">\n      </div>\n        <input type='text' value={formInput}\n        onChange={(e) => setFormInput(e.target.value)}\n        />\n        <button onClick={onSearchButtonClick}>\n          Search job\n        </button>\n        <div>\n          {/* only one display right now, using findone */}\n          {jobList}\n\n        </div>\n        </div>\n      </div>\n    </div>\n  );\n\n}\n\nexport default LoggedHome;\n","/Users/ziling/Desktop/job-listing-board/src/js/PostJob.jsx",["66"],"import React,{useState, useEffect} from \"react\";\nimport \"../css/PostJob.css\";\nimport axios from \"axios\";\nimport { useNavigate } from 'react-router';\nimport Nav from \"./Nav\";\n\nfunction PostJob(props) {\n    const [jobData, setJobData] = useState({\n        title: '',\n        company: '',\n        location: '',\n        description: '',\n        employerEmail: '',\n        web: '',\n        postUser: '',\n    });\n    const [error, setError] = useState(\"\");\n\n    const navigate = useNavigate();\n    const navigateLogin = useNavigate();\n    function tryPostJob() {\n        axios.post('/api/jobs/add', jobData)\n            .then((jobResponse) => {\n                navigate('/jobDetail/'+jobResponse.data._id);\n            })\n            .catch(e => setError(e.response.data));\n    }\n\n    \n    function addToMyList() {\n        axios.get(\"/api/users/whoIsLoggedIn\")\n            .then((userRes) => {\n                const username = userRes.data;\n                setJobData({\n                    ...jobData,\n                    postUser: username\n                })\n            })\n            .catch(() => navigateLogin('/logIn'));\n    }\n    useEffect(addToMyList, []);\n\n    return(\n        <div className=\"page\">\n            <Nav type = \"postjob\" />\n\n        <div className = \"contain\">\n            <div className=\"job-finder\">\n                JobFinder:\n                <p>{error}</p>\n            </div>\n\n            <div className = \"username\">\n                Title*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.title} onChange={(e) => {\n                const title = e.target.value;\n                setJobData({\n                    ...jobData,\n                    title: title\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Company Name*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.company} onChange={(e) => {\n                const company = e.target.value;\n                setJobData({\n                    ...jobData,\n                    company: company\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Location*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.location} onChange={(e) => {\n                const location = e.target.value;\n                setJobData({\n                    ...jobData,\n                    location: location\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Description*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.description} onChange={(e) => {\n                const description = e.target.value;\n                setJobData({\n                    ...jobData,\n                    description: description\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Employer Email*:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.employerEmail} onChange={(e) => {\n                const employerEmail = e.target.value;\n                setJobData({\n                    ...jobData,\n                    employerEmail: employerEmail\n                })\n            }}/>\n            </div>\n            <div className = \"username\">\n                Company Website(Optional):\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={jobData.web} onChange={(e) => {\n                const web = e.target.value;\n                setJobData({\n                    ...jobData,\n                    web: web\n                })\n            }}/>\n            </div>\n\n            <button onClick={tryPostJob}>Post Job</button>\n        </div>\n        </div> \n    );\n\n}\n\nexport default PostJob","/Users/ziling/Desktop/job-listing-board/src/js/LogIn.jsx",["67","68"],"import axios from 'axios';\nimport React, {useState} from 'react';\nimport Nav from './Nav';\nimport \"../css/LogIn.css\";\nimport { useNavigate } from 'react-router'; \nimport { Link } from 'react-router-dom';\n\n\nfunction LogIn(props) {\n\n    const [userData, setUserData] = useState({\n        password: '',\n        username: '',\n    });\n    const [error, setError] = useState(\"\");\n\n   \n    const navigate = useNavigate();\n    function tryLogin () {\n        axios.post('/api/users/login', userData)\n                .then((loginResponse) => {\n                    navigate('/userLogged', {state: loginResponse.data.username});\n                })\n                .catch(e => setError(e.response.data));    \n    }\n    \n\n    return (\n        <div className=\"page\">\n        <div>\n                <Link to=\"/\">\n                <button>Home</button>\n                </Link>\n                <Link to=\"/register\">\n                <button>Register</button>\n                </Link>\n            </div>\n        <div className = \"contain\">\n            <div className=\"job-finder\">\n                JobFinder\n            </div>\n            <div className = \"username\">\n                Username:\n                <p>{error}</p>\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={userData.username} onChange={(e) => {\n                const username = e.target.value;\n                setUserData({\n                    ...userData,\n                    username: username\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Password:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={userData.password} onChange={(e) => {\n                const password = e.target.value;\n                setUserData({\n                    ...userData,\n                    password: password\n                })\n            }} type='password' />\n            </div>\n\n            <button onClick={tryLogin}>Login</button>\n           \n        </div>\n        </div>    \n    );\n}\n\nexport default LogIn","/Users/ziling/Desktop/job-listing-board/src/js/Favorite.jsx",["69","70"],"import React, { useState } from \"react\";\nimport axios from \"axios\";\nimport { useEffect } from \"react\";\nimport { Link } from 'react-router-dom';\nimport { useNavigate } from 'react-router';\n\n\nexport default function Favorite(props){\n    const navigate = useNavigate();\n    const [fav, setFav] = useState([])\n\n\n    function checkLogin() {\n        axios.get('/api/users/whoIsLoggedIn')\n            .then((response) => {\n                axios.get(\"/api/users/need/\"+response.data)\n                .then((favResponse) => {\n                    setFav(favResponse.data.favorites)\n                    console.log(fav);\n                })\n                .catch(error => console.log(error))\n            })\n            .catch(() => navigate('/logIn'))\n    }\n\n    useEffect(checkLogin,[]);\n\n    \n\n    const result = fav.map(one => {\n        axios.get(\"/api/jobs/jobDetail/\"+one)\n            .then((response)=> {\n                    return (<div>\n                    <Link to={\"/jobDetail/\"+response.data._id}>\n                    <div>\n                    <p>Job Title: {response.data.title}</p>\n                    <p>Company: {response.data.company}</p>\n                    <p>Location: {response.data.location}</p>\n                    </div>\n                    </Link>\n                    <br/>\n                    </div>)})\n            .catch(error => console.log(error))\n        })\n\n    \n        return (\n            <div>\n                {result}\n            </div>\n        )\n\n}","/Users/ziling/Desktop/job-listing-board/src/js/MyList.jsx",["71"],"import React, {useState, useEffect} from \"react\";\nimport axios from 'axios';\nimport { Link } from 'react-router-dom';\nimport {useNavigate} from 'react-router';\n\n\nexport default function MyList(props) {\n\n\n    const [job, setJob] = useState([]);\n\n    const navigate = useNavigate();\n    function tryEdit (id) {\n        navigate(\"/editJob/\" + id);\n    }\n\n    const navigateBack = useNavigate();\n    function tryDelete (id) {\n        axios.delete(\"/api/jobs/delete/\" + id)\n            .then(response => {\n                navigateBack(\"/userLogged\");\n                window.location.reload();\n            })\n            .catch(error => console.log(error));\n    }\n\n    function findLogInUserAndSetJobList () {\n        axios.get(\"/api/users/whoIsLoggedIn\")\n            .then(userResponse => {\n                axios.get(\"/api/jobs/aJob/\" + userResponse.data)\n                    .then((listResponse) => setJob(listResponse.data))\n                    .catch(error => console.log(error))\n            })\n            .catch(() => navigate('/logIn'))\n    }\n    useEffect(findLogInUserAndSetJobList, []);\n\n\n    const jobList = job.map(oneJob => {\n        return (\n            \n                <div id = \"joblst\">\n                <button onClick={() => {tryEdit(oneJob._id)}}>Edit</button>\n                <button onClick={() => {tryDelete(oneJob._id)}}>Delete</button>\n                <Link to={\"/jobDetail/\"+oneJob._id}>\n                \n                <div>\n                  <p key=\"uniqueId1\">Job Title: {oneJob.title}</p>\n                  <p key=\"uniqueId2\">Company: {oneJob.company}</p>\n                  <p key=\"uniqueId3\">Location: {oneJob.location}</p>\n                </div>\n                </Link>\n                <br/>\n                </div>)\n      })\n\n    return (\n        <div> \n            <div>\n            {jobList.length === 0 && <p>No Post yet</p>}\n            </div>\n            {jobList}\n        </div>\n    )\n}","/Users/ziling/Desktop/job-listing-board/src/js/Register.jsx",["72","73"],"import axios from 'axios';\nimport React, {useState} from 'react'\nimport \"../css/LogIn.css\";\nimport { useNavigate } from 'react-router'; \nimport { Link } from 'react-router-dom';\n\n\nfunction Register(state) {\n    const [userData, setUserData] = useState({\n        username: '',\n        password: '',\n        verifyPassword: '',\n    });\n    const [error, setError] = useState(\"\");\n\n    \n    const navigate = useNavigate();\n    function tryRegister () {\n        axios.post('/api/users/register', userData)\n                .then((registerResponse) => {\n                    navigate('/userLogged', {state: registerResponse});\n                    })\n                .catch(e => setError(e.response.data));\n    }\n\n    return (\n        <div className=\"page\">\n        <div>\n                <Link to=\"/\">\n                <button>Home</button>\n                </Link>\n                <Link to=\"/login\">\n                <button>Log In</button>\n                </Link>\n            </div>\n\n        <div className = \"contain\">\n            <div className=\"job-finder\">\n                Register new account\n            </div>\n            <div className = \"username\">\n                Username:\n                <p>{error}</p>\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={userData.username} onChange={(e) => {\n                const username = e.target.value;\n                setUserData({\n                    ...userData,\n                    username: username\n                })\n            }}/>\n            </div>\n\n            <div className = \"username\">\n                Password:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={userData.password} onChange={(e) => {\n                const password = e.target.value;\n                setUserData({\n                    ...userData,\n                    password: password\n                })\n            }} type='password' />\n            </div>\n\n            <div className = \"username\">\n                Verify Password:\n            </div>\n            <div className = \"input-box\">\n            <input type='text' value={userData.verifyPassword} onChange={(e) => {\n                const verifyPassword = e.target.value;\n                setUserData({\n                    ...userData,\n                    verifyPassword: verifyPassword\n                })\n            }} type='password' />\n            </div>\n            <button onClick={tryRegister}>Register</button>\n\n        </div>\n        </div>    \n    );\n}\n\nexport default Register","/Users/ziling/Desktop